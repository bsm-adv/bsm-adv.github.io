---
import Layout from "../layouts/Layout.astro";
import Header from "../components/site/Header.astro";
import Footer from "../components/site/Footer.astro";
import Container from "../components/site/Container.astro";
import Title from "../components/site/Title.astro";
import Card from "../components/contact/Card.astro";
import Hr from "../components/site/Hr.astro";

import alamat from "../data/alamat.json";
import Content from "../components/contact/Content.astro";
const items = Object.values(alamat);
---

<Layout
    title="Contact Us | Bintang Surya Madani"
    description="Whatever your goal - we will get you there"
>

    <Container class="px-4 py-2">
        <Header/>
    </Container>

    <Container class="px-4 py-24 max-w-none" imgUrl="https://picsum.photos/id/619/1920/480">
        <Container class="text-center relative z-10">
            <Title
                title="Contact Us"
                description="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua."
                class="text-yellow-500"
                subclass="text-neutral-300"
            />
        </Container>
    </Container>

    <Container class="px-4 pb-24 md:py-24 max-w-(--breakpoint-lg)">
        <Content/>
    </Container>
    <!-- <iframe class="absolute inset-0 w-full h-full -z-10" src="https://maps.google.com/maps?width=1920&height=1080&hl=en&q=cv.%20bintang%20surya%20madani&t=&z=15&ie=UTF8&iwloc=B&output=embed"></iframe> -->

    <Hr/>

    <Footer/>

</Layout>
